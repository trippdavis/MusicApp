<form action="<%= track_notes_url(track) %>" method="post">
  <input
    type="hidden"
    name="authenticity_token"
    value="<%= form_authenticity_token %>">

  <textarea
    name="note[body]"
    rows="8"
    cols="40"
    placeholder="Enter note here"></textarea>
  <br>

  <input type="submit" name="Submit">
</form>
